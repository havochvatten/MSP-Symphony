{
  "name": "symphony-fe",
  "version": "1.23.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --ssl",
    "build": "ng build",
    "prod-build": "ng build --configuration production",
    "test": "ng test --code-coverage=true",
    "test-headless": "ng test --watch=false --code-coverage=true --browsers=ChromeCI",
    "test-headless-win": "ng test --watch=false --code-coverage=true --browsers=ChromeHeadless",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "format": "prettier \"src/**/*.{js,json,ts,html,css,less}\" --write",
    "lint:ts": "tslint -p tsconfig.json",
    "lint:ts-autofix": "tslint --fix -p tsconfig.json",
    "precommit": "lint-staged",
    "prepush": "npm run lint:ts",
    "generate-cert": "openssl req -newkey rsa:2048 -x509 -nodes -keyout ssl/server.key -new -out ssl/server.crt -config ssl/openssl-custom.cnf -sha256 -days 365",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.11",
    "@angular/cdk": "17.3.10",
    "@angular/common": "^17.3.11",
    "@angular/compiler": "^17.3.11",
    "@angular/core": "^17.3.11",
    "@angular/forms": "^17.3.11",
    "@angular/material": "^17.3.10",
    "@angular/platform-browser": "^17.3.11",
    "@angular/platform-browser-dynamic": "^17.3.11",
    "@angular/router": "^17.3.11",
    "@fortawesome/angular-fontawesome": "^0.14.1",
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@ngrx/effects": "^17.2.0",
    "@ngrx/store": "^17.2.0",
    "@ngrx/store-devtools": "^17.2.0",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "@turf/boolean-equal": "^6.5.0",
    "@turf/boolean-intersects": "^6.5.0",
    "@turf/clean-coords": "^6.5.0",
    "@turf/difference": "^6.5.0",
    "@turf/helpers": "^6.5.0",
    "@turf/intersect": "^6.5.0",
    "@turf/union": "^6.5.0",
    "@types/arcgis-rest-api": "^10.4.8",
    "@types/proj4": "^2.5.5",
    "@types/topojson-specification": "^1.0.5",
    "core-js": "^3.37.1",
    "d3": "^7.9.0",
    "d3-sankey": "^0.12.3",
    "immutable": "^4.3.6",
    "karma-coverage": "^2.2.1",
    "ol": "^9.2.2",
    "proj4": "^2.11.0",
    "rxjs": "^7.8.1",
    "tslib": "^2.6.2",
    "uuid": "^9.0.1",
    "zone.js": "^0.14.6"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.17",
    "@angular-eslint/builder": "17.5.3",
    "@angular-eslint/eslint-plugin": "17.5.3",
    "@angular-eslint/eslint-plugin-template": "17.5.3",
    "@angular-eslint/schematics": "17.5.3",
    "@angular-eslint/template-parser": "17.5.3",
    "@angular/cli": "^17.3.17",
    "@angular/compiler-cli": "^17.3.12",
    "@angular/language-service": "^17.3.12",
    "@babel/core": "^7.24.5",
    "@types/d3": "^7.4.3",
    "@types/d3-sankey": "^0.12.4",
    "@types/geojson": "^7946.0.14",
    "@types/jasmine": "~5.1.4",
    "@types/jasminewd2": "~2.0.13",
    "@types/node": "^20.12.12",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "7.9.0",
    "@typescript-eslint/parser": "7.9.0",
    "angular-in-memory-web-api": "^0.17.0",
    "autoprefixer": "^10.4.19",
    "babel-loader": "^9.1.3",
    "eslint": "^9.3.0",
    "jasmine-core": "~5.1.2",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.3",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "lint-staged": "^15.2.2",
    "postcss": ">=8.4.38",
    "prettier": "^3.2.5",
    "raw-loader": "^4.0.2",
    "ts-node": "~10.9.2",
    "typescript": "5.4.5"
  },
  "overrides": {
    "axios": ">=1.7.7"
  },
  "optionalDependencies": {
    "@nx/nx-darwin-arm64": "19.0.4",
    "@nx/nx-darwin-x64": "19.0.4",
    "@nx/nx-linux-x64-gnu": "19.0.4",
    "@nx/nx-win32-x64-msvc": "19.0.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit",
      "pre-push": "npm run prepush"
    }
  },
  "lint-staged": {
    "*.{js,json,ts,html,css,less}": [
      "prettier --write",
      "git add"
    ]
  }
